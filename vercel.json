{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" },
      { "src": "public/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/fetchCarData", "dest": "/api/fetchCarData" },
      { "src": "/api/fetchAdvancedCarData", "dest": "/api/fetchAdvancedCarData" },
      { "src": "/api/fetchRewardsHistory", "dest": "/api/fetchRewardsHistory" },
      { "src": "/token/(.*)", "dest": "/index.html" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  